{
  "name": "dh12-notetaking-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:tunnel": "npm run tunnel:check && concurrently -n \"nextjs,tunnel\" -c \"blue,green\" \"npm run dev\" \"npm run tunnel:wait\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "convex:dev": "convex dev",
    "convex:deploy": "convex deploy",
    "tunnel": "cloudflared tunnel --config cloudflared-config.yml run",
    "tunnel:wait": "node scripts/wait-and-tunnel.js",
    "tunnel:login": "cloudflared tunnel login",
    "tunnel:create": "cloudflared tunnel create DH12",
    "tunnel:route": "cloudflared tunnel route dns DH12 DH.nicholasching.ca",
    "tunnel:check": "node scripts/check-tunnel-prereqs.js",
    "tunnel:preflight": "npm run tunnel:check && npm run tunnel"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.36.7",
    "@google/generative-ai": "^0.24.1",
    "@tiptap/extension-code-block-lowlight": "^2.27.2",
    "@tiptap/extension-underline": "^2.27.2",
    "@tiptap/react": "^2.1.0",
    "@tiptap/starter-kit": "^2.1.0",
    "assemblyai": "^4.0.0",
    "clsx": "^2.1.0",
    "convex": "^1.12.0",
    "date-fns": "^4.1.0",
    "groq-sdk": "^0.37.0",
    "lowlight": "^3.3.0",
    "lucide-react": "^0.562.0",
    "next": "^16.1.1",
    "qrcode": "^1.5.3",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-draggable": "^4.5.0",
    "react-qr-code": "^2.0.12",
    "tailwind-merge": "^2.2.0",
    "tldraw": "^2.1.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20.11.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "latest",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.4.0"
  }
}
